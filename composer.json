{
    "name": "balt-technologies/facility-service-cloud-api-client",
    "description": "API Client / SDK for https://www.facilityservice.cloud",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Redmann",
            "email": "tobias.redmann@balt.de"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.2",
        "netresearch/jsonmapper": "^4.0",
        "php-di/php-di": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "FacilityCloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
